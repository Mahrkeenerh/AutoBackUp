{
    "sources": [
        "~/Documents",
        "~/Pictures"
    ],
    "lists": [
        "~/.config",
        "~/.local/bin"
    ],
    "destination": "~/Backups",
    "destination_format": "%Y-%m-%d",
    "repeat": true,
    "time_format": "%H",
    "time_value": "19",
    "sleep": 3600,
    "keep_copies": 7
}
